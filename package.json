{
  "name": "qmobile-api",
  "version": "1.0.0",
  "description": "Servidor do QMobile",
  "main": "dist/qmobile.js",
  "scripts": {
    "lint": "tslint --project .",
    "test": "cross-env NODE_ENV=test jasmine-ts \"./src/**/*.spec.ts\"",
    "test:cov": "cross-env NODE_ENV=test nyc npm test",
    "test:ci": "npm run test:cov && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "start:dev": "ts-node ./src/api/index.ts",
    "start": "node ./src/api/index.js",
    "build": "tsc --project ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alan-Gomes/qmobile-api.git"
  },
  "keywords": [
    "qmobile",
    "api",
    "app",
    "aluno",
    "estudante"
  ],
  "author": "Alan-Gomes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alan-Gomes/qmobile-api/issues"
  },
  "homepage": "https://github.com/Alan-Gomes/qmobile-api#readme",
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "body-parser": "^1.18.2",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.1.2",
    "cors": "^2.8.4",
    "dataloader": "^1.4.0",
    "express": "^4.15.5",
    "express-fileupload": "^0.4.0",
    "fs-extra": "^5.0.0",
    "generic-pool": "^3.4.1",
    "graphql": "^0.13.1",
    "graphql-tools": "^2.21.0",
    "iconv-lite": "^0.4.19",
    "kue": "^0.11.6",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "node-cron": "^1.2.1",
    "promise-mysql": "^3.2.1",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "sharp": "^0.18.4",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.3",
    "@types/colors": "^1.1.3",
    "@types/cors": "^2.8.3",
    "@types/express": "^4.0.37",
    "@types/express-fileupload": "^0.1.1",
    "@types/fs-extra": "^5.0.0",
    "@types/generic-pool": "^3.1.0",
    "@types/graphql": "^0.12.5",
    "@types/image-size": "0.0.29",
    "@types/jasmine": "^2.8.6",
    "@types/kue": "^0.11.6",
    "@types/lodash": "^4.14.104",
    "@types/lodash.uniqby": "^4.7.3",
    "@types/node-cron": "^1.2.0",
    "@types/request-promise": "^4.1.41",
    "@types/sharp": "^0.17.4",
    "@types/tough-cookie": "^2.3.2",
    "@types/uuid": "^3.4.3",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "image-size": "^0.6.2",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "nyc": "^11.4.1",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "exclude": [
      "src/**/*.spec.ts",
      "src/**/*.js",
      "test/**/*"
    ],
    "temp-directory": "./coverage/.nyc_output",
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".ts"
    ]
  }
}
